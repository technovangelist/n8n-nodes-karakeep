{
  "name": "n8n-nodes-karakeep",
  "version": "1.0.0",
  "description": "n8n node for Karakeep bookmark management service",
  "keywords": [
    "n8n-community-node-package",
    "karakeep",
    "bookmarks",
    "n8n",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://github.com/your-username/n8n-nodes-karakeep",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/n8n-nodes-karakeep.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s",
    "test": "jest",
    "link:n8n": "npm run build && npm link && cd ~/.n8n && npm link n8n-nodes-karakeep",
    "unlink:n8n": "cd ~/.n8n && npm unlink n8n-nodes-karakeep && npm unlink -g n8n-nodes-karakeep"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KarakeepApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Karakeep/Karakeep.node.js",
      "dist/nodes/KarakeepTest/KarakeepTest.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.15.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "^1.15.0"
  }
}