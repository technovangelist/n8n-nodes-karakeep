{
  "name": "Karakeep Lists Management Workflow",
  "nodes": [
    {
      "parameters": {},
      "id": "start",
      "name": "Start",
      "type": "n8n-nodes-base.start",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "resource": "lists",
        "operation": "create",
        "name": "JavaScript Learning Resources",
        "icon": "âš¡",
